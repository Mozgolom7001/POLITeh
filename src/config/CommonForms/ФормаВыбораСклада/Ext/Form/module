Процедура ПриОткрытии()

	СписокСкладов = НачальноеЗначениеВыбора.СписокСкладов;
	НачЗначение   = НачальноеЗначениеВыбора.НачЗначение;
	
	ЭлементыФормы.СписокСкладов.СписокВыбора = СписокСкладов;
	Если СписокСкладов.НайтиПоЗначению(НачЗначение) <> Неопределено Тогда
		ЭлементыФормы.СписокСкладов.Значение = НачЗначение;
	Иначе
		Если НачальноеЗначениеВыбора.Количество() > 1 Тогда
			ЭлементыФормы.СписокСкладов.Значение = ЭлементыФормы.СписокСкладов.СписокВыбора[0].Значение;
		КонецЕсли;
	КонецЕсли;

КонецПроцедуры

// Данная форма не должна открываться интерактивно
//
Процедура ПередОткрытием(Отказ, СтандартнаяОбработка)

	Если НачальноеЗначениеВыбора = Неопределено Тогда
		Отказ = Истина;
	КонецЕсли;
	
КонецПроцедуры // ПередОткрытием()

Процедура ОсновныеДействияФормыОК(Кнопка)

	Закрыть(Склад);
	
КонецПроцедуры
