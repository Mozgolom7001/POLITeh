				ЗаполнитьСвойствоXDTO(АдрРФ, "Корпус",   ЗначениеРеквизитаВДереве(ДеревоДанных, "УчастЭДО.ТипУчастника.ЮЛ.АдрРФ.Корпус"), , ТекстОшибки);
				ЗаполнитьСвойствоXDTO(АдрРФ, "Кварт",   ЗначениеРеквизитаВДереве(ДеревоДанных, "УчастЭДО.ТипУчастника.ЮЛ.АдрРФ.Кварт"), , ТекстОшибки);
				ЗаполнитьСвойствоXDTO(СвЮЛ, "АдрРФ", АдрРФ, , ТекстОшибки);
				
				ФИООтв = ПолучитьОбъектТипаCML(ПутьКОписанию + ".Документ.УчастЭДО.ЮЛ.ФИООтв");
				ЗаполнитьСвойствоXDTO(ФИООтв, "Фамилия",  ЗначениеРеквизитаВДереве(ДеревоДанных, "УчастЭДО.ТипУчастника.ЮЛ.ФИООтв.Фамилия"), Истина, ТекстОшибки);
				ЗаполнитьСвойствоXDTO(ФИООтв, "Имя",      ЗначениеРеквизитаВДереве(ДеревоДанных, "УчастЭДО.ТипУчастника.ЮЛ.ФИООтв.Имя"), Истина, ТекстОшибки);
				ЗаполнитьСвойствоXDTO(ФИООтв, "Отчество", ЗначениеРеквизитаВДереве(ДеревоДанных, "УчастЭДО.ТипУчастника.ЮЛ.ФИООтв.Отчество"), , ТекстОшибки);
				ЗаполнитьСвойствоXDTO(СвЮЛ, "ФИООтв", ФИООтв, , ТекстОшибки);
				
				ЗаполнитьСвойствоXDTO(УчастникЭДО, "ЮЛ", СвЮЛ, , ТекстОшибки);
