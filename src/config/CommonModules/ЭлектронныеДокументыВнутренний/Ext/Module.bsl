					КонецЕсли;
					
					СтрокаТаблицы = ТекстоваяИнформация.Добавить();
					СтрокаТаблицы.Идентификатор = СтрокаИнформации.Идентиф;
					СтрокаТаблицы.Значение      = СтрокаИнформации.Значен;
