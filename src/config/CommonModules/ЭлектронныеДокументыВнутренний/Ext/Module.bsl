// Проверка дерева ЭД (для проверки выполнения определенных условий на заполненность реквизитов)

Процедура ПроверитьИнформациюПродавца(ДеревоДанных, СтруктураЭД, ТекстОшибки)

	Ошибки = Новый Массив;
	ФункцияУПД = "";
	СтруктураЭД.Свойство("Функция", ФункцияУПД);
	
	// Сведения о товарах
	СтрокаТаблицаТоваров = ДеревоДанных.Строки.Найти("СведенияОТоварах", "ПолныйПуть");
	НомерСтроки = 1;
	Для Каждого Товар Из СтрокаТаблицаТоваров.Строки Цикл
		ЕдиницаИзмеренияКод = ОбщегоНазначенияЭД.ЗначениеРеквизитаВДереве(Товар,
			"СведенияОТоварах.НомерСтроки.ЕдиницаИзмеренияКод");
		Количество = ОбщегоНазначенияЭД.ЗначениеРеквизитаВДереве(Товар,
			"СведенияОТоварах.НомерСтроки.Количество");
		
		
		Если (ФункцияУПД = "СЧФДОП" ИЛИ ФункцияУПД = "ДОП") И ЗначениеЗаполнено(ЕдиницаИзмеренияКод)
			И НЕ ТолькоСимволыВСтроке(ЕдиницаИзмеренияКод, "0") И Не ЗначениеЗаполнено(Количество) Тогда
			ИмяПоля = "Количество";
			ИмяТаблицы = "СведенияОТоварах";
			
			ШаблонОшибки = НСтр("ru = 'Не заполнено обязательное поле ""%1"" таблицы ""%2"" в строке %3'");
			ЭлементОшибки = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(ШаблонОшибки, ИмяПоля, ИмяТаблицы, НомерСтроки);
			Ошибки.Добавить(ЭлементОшибки);
		КонецЕсли;
		
		ТоварНаименование = ОбщегоНазначенияЭД.ЗначениеРеквизитаВДереве(Товар,
			"СведенияОТоварах.НомерСтроки.ТоварНаименование");
		
		Если СтрДлина(ТоварНаименование) > 1000 Тогда
			
			СтрокаТоварНаименование = Товар.Строки.Найти("СведенияОТоварах.НомерСтроки.ТоварНаименование", "ПолныйПуть");
			
			ОписаниеОграничений = СтрокаТоварНаименование.ОписаниеОграничений;
			ИмяПоля = ЭлектронныеДокументыПовтИсп.ПрикладноеПредставлениеРеквизита(СтрокаТоварНаименование.КодПредставления);
			ИмяТаблицы = ЭлектронныеДокументыПовтИсп.ПрикладноеПредставлениеРеквизита(СтрокаТаблицаТоваров.КодПредставления);
			
			ШаблонОшибки = НСтр("ru = 'Нарушено ограничение ""%1"" для поля ""%2"" таблицы ""%3"" в строке %4'");
			ЭлементОшибки = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(ШаблонОшибки, 
				ОписаниеОграничений, ИмяПоля, ИмяТаблицы, НомерСтроки);
			Ошибки.Добавить(ЭлементОшибки);
			
		КонецЕсли;
		
		НомерСтроки = НомерСтроки + 1;
	КонецЦикла;
	
	Для Каждого ЭлементОшибки Из Ошибки Цикл
		ТекстОшибки = ТекстОшибки + ЭлементОшибки + Символы.ПС;
	КонецЦикла;

КонецПроцедуры

Процедура ПроверитьПередачаРаботИсполнитель(ДеревоДанных, СтруктураЭД, ТекстОшибки)
	
	Ошибки = Новый Массив;
	
	// Таблица услуг
	СтрокаТаблицаУслуг = ДеревоДанных.Строки.Найти("ТаблицаУслуг", "ПолныйПуть");
	НомерСтроки = 1;
	Для Каждого Услуга Из СтрокаТаблицаУслуг.Строки Цикл
		
		УслугаНаименованиеНоменклатуры = ОбщегоНазначенияЭД.ЗначениеРеквизитаВДереве(Услуга,
			"ТаблицаУслуг.НомерСтроки.НаименованиеНоменклатуры");
		
		Если СтрДлина(УслугаНаименованиеНоменклатуры) > 1000 Тогда
			
			СтрокаУслугаНаименованиеНоменклатуры = Услуга.Строки.Найти("ТаблицаУслуг.НомерСтроки.НаименованиеНоменклатуры", "ПолныйПуть");
			
			ОписаниеОграничений = СтрокаУслугаНаименованиеНоменклатуры.ОписаниеОграничений;
			ИмяПоля = ЭлектронныеДокументыПовтИсп.ПрикладноеПредставлениеРеквизита(СтрокаУслугаНаименованиеНоменклатуры.КодПредставления);
			ИмяПоля = ?(ЗначениеЗаполнено(ИмяПоля), ИмяПоля, НСтр("ru = 'Наименование услуги'"));
			ИмяТаблицы = ЭлектронныеДокументыПовтИсп.ПрикладноеПредставлениеРеквизита(СтрокаТаблицаУслуг.КодПредставления);
			
			ШаблонОшибки = НСтр("ru = 'Нарушено ограничение ""%1"" для поля ""%2"" таблицы ""%3"" в строке %4'");
			ЭлементОшибки = СтроковыеФункцииКлиентСервер.ПодставитьПараметрыВСтроку(ШаблонОшибки, 
				ОписаниеОграничений, ИмяПоля, ИмяТаблицы, НомерСтроки);
			Ошибки.Добавить(ЭлементОшибки);
			
		КонецЕсли;
		
		НомерСтроки = НомерСтроки + 1;
	КонецЦикла;
	
	Для Каждого ЭлементОшибки Из Ошибки Цикл
		ТекстОшибки = ТекстОшибки + ЭлементОшибки + Символы.ПС;
	КонецЦикла;

КонецПроцедуры

