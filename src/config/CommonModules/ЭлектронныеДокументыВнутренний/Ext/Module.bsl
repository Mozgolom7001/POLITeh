				ЗаполнитьСвойствоXDTO(АдрРФ, "Район",   ЗначениеРеквизитаВДереве(ДеревоДанных, "УчастЭДО.ТипУчастника.ЮЛ.АдрРФ.Район"), , ТекстОшибки);
				ЗаполнитьСвойствоXDTO(АдрРФ, "Город",   ЗначениеРеквизитаВДереве(ДеревоДанных, "УчастЭДО.ТипУчастника.ЮЛ.АдрРФ.Город"), , ТекстОшибки);
				ЗаполнитьСвойствоXDTO(АдрРФ, "НаселПункт",   ЗначениеРеквизитаВДереве(ДеревоДанных, "УчастЭДО.ТипУчастника.ЮЛ.АдрРФ.НаселПункт"), , ТекстОшибки);
				ЗаполнитьСвойствоXDTO(АдрРФ, "Улица",   ЗначениеРеквизитаВДереве(ДеревоДанных, "УчастЭДО.ТипУчастника.ЮЛ.АдрРФ.Улица"), , ТекстОшибки);
