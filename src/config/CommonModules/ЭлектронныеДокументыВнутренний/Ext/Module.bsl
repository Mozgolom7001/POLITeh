			ДобавитьРеквизитШапкиОбъекта(НовыйЭД, "Валюта", НайденнаяСтрока.ИндексСтроки);
		КонецЕсли;
		
		Курс = ЭД.Курс;
		Если Курс <> Неопределено Тогда
			ДобавитьРеквизитШапкиОбъекта(НовыйЭД, "Курс", Курс);
