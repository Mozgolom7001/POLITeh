	СтруктураВсего.Вставить("МассаНетто");
	СтруктураВсего.Вставить("СуммаБезНДС");
	СтруктураВсего.Вставить("СуммаНДС");
	СтруктураВсего.Вставить("СуммаСНДС");
	
	Возврат СтруктураВсего;
	
КонецФункции

