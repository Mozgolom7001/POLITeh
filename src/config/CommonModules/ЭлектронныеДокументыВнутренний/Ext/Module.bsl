Функция НаименованиеПервичногоДокументаВФайле(Знач ФайлЭД)
	
	Результат = "";
	
	Попытка
		
		СвойстваСохраненногоФайла = СвойстваФайла();
		СохранитьЭДВФайл(ФайлЭД, СвойстваСохраненногоФайла);
		
		ЧтениеXML = Новый ЧтениеXML;
		ЧтениеXML.ОткрытьФайл(СвойстваСохраненногоФайла.ИмяФайла);
		ДанныеФайла = ФабрикаXDTO.ПрочитатьXML(ЧтениеXML);
		ЧтениеXML.Закрыть();
		
		Если ТипЗнч(ДанныеФайла) <> Тип("ОбъектXDTO") Тогда
			ВызватьИсключение НСтр("ru = 'Не удалось прочитать файл электронного документа.'");
		КонецЕсли;
		
		ФорматКорректен = Ложь;
		
		Если ЕстьСвойствоXDTO(ДанныеФайла, "Документ") Тогда
			Если ЕстьСвойствоXDTO(ДанныеФайла.Документ, "НаимДокОпр") Тогда
				Результат = ДанныеФайла.Документ.НаимДокОпр;
				ФорматКорректен = Истина;
			КонецЕсли;
		КонецЕсли;
		
		Если Не ФорматКорректен Тогда
			ВызватьИсключение НСтр("ru = 'Некорректный формат файла электронного документа.'");
		КонецЕсли;
		
	Исключение
		
		ТекстОшибки = НСтр("ru = 'Не удалось получить наименование первичного документа по причине:'") 
			+ Символы.ПС + ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
		ЭлектронныеДокументыСлужебный.ВыполнитьЗаписьСобытияПоЭДВЖурналРегистрации(ТекстОшибки,
			2,
			УровеньЖурналаРегистрации.Ошибка,
			ФайлЭД.Метаданные(),
			ФайлЭД);
		
		Результат = Неопределено;
		
	КонецПопытки;
	
	Если ЗначениеЗаполнено(СвойстваСохраненногоФайла.ИмяФайла) Тогда
		
		Попытка
			УдалитьФайлы(СвойстваСохраненногоФайла.ИмяФайла);
		Исключение
			ТекстОшибки = НСтр("ru = 'Не удалось удалить временный файл:'")
				+ Символы.ПС + СвойстваСохраненногоФайла.ИмяФайла
				+ Символы.ПС + ПодробноеПредставлениеОшибки(ИнформацияОбОшибке());
			ЭлектронныеДокументыСлужебный.ВыполнитьЗаписьСобытияПоЭДВЖурналРегистрации(ТекстОшибки, 2);
		КонецПопытки;
		
	КонецЕсли;
	
	Возврат Результат;
	
КонецФункции

