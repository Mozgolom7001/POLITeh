				АдрРФ = ПолучитьОбъектТипаCML(ПутьКОписанию + ".Документ.УчастЭДО.ИП.АдрРФ");
				ЗаполнитьСвойствоXDTO(АдрРФ, "Индекс",   ЗначениеРеквизитаВДереве(ДеревоДанных, "УчастЭДО.ТипУчастника.ИП.АдрРФ.Индекс"), , ТекстОшибки);
